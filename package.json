{
  "name": "metajelo-web",
  "version": "0.1.0",
  "description": "A Starter kit for Purescript-Concur. Uses Parcel.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf output && rimraf .pulp-cache",
