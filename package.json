{
  "name": "purescript-metajelo-web",
  "description": "Display component for Metajelo xml files",
  "main": "static/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "testbrowser": "./scripts/testbrowser",
    "clean": "rimraf output && rimraf .pulp-cache && rm -fr dist",
    "build": "bower install && pulp build -- --censor-lib --strict",
    "ps": "bower install && pulp build",
    "prod": "npm run ps && parcel build --public-url ./ static/index.html",
    "start": "parcel --no-hmr --public-url ./ static/index.html",
    "watch": "parcel watch static/index.html"
  },
  "author": "Brandon Barker <brandon.barker@cornell.edu> (https://github.com/bbarker)",
  "license": "MPL2",
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "bower": "^1.8.6",
    "cssmin": "^0.4.3",
    "parcel-bundler": "^1.12.3",
    "postcss-font-base64": "^1.0.5",
    "postcss-modules": "^1.4.1",
    "pulp": "^12.3.1",
    "purescript": "^0.12.3",
    "purescript-psa": "^0.7.3",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "browserslist": [
    "defaults"
  ]
}
